<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TrackMobile.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TrackMobile"
    Shell.FlyoutBehavior="Flyout"
    Title="TrackMobile">

    <ShellContent
     Title="Appareils"
     ContentTemplate="{DataTemplate local:APPAPVIS}"
     Route="APPAPVIS" />
    
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

   

</Shell>
